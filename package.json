{
  "name": "syncio-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "deploy-beta": "vite build --mode beta && aws s3 rm s3://syncio-v2-beta-app.syncio.io --recursive && aws s3 sync ./dist s3://syncio-v2-beta-app.syncio.io",
    "deploy-production": "vite build --mode production && aws s3 rm s3://app-v2.syncio.co --recursive && aws s3 sync ./dist s3://app-v2.syncio.co && aws-cloudfront-invalidate E2XTG3KY1VIR4",
    "deploy-staging": "NODE_ENV=development vite build --mode staging && aws s3 rm s3://staging-app.syncio.io --recursive && aws s3 sync ./dist s3://staging-app.syncio.io && aws-cloudfront-invalidate E1JQLAWU0IBM3T",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "format": "prettier --write \"./src/**/*.{js}\""
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^4.2.3",
    "@vueuse/components": "^10.7.0",
    "@vueuse/core": "^10.7.0",
    "animated-number-vue": "^1.0.0",
    "aws-cloudfront-invalidate": "^0.1.3",
    "aws-s3": "^2.0.5",
    "axios": "^0.27.2",
    "deepmerge": "^4.2.2",
    "json-server": "^0.17.0",
    "luxon": "^3.3.0",
    "pinia": "^2.1.3",
    "pinia-plugin-persist": "^1.0.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.32.2",
    "sass": "^1.53.0",
    "vee-validate": "^4.12.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue3-toastify": "^0.1.12",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/test-utils": "^2.0.2",
    "jsdom": "^20.0.0",
    "start-server-and-test": "^1.14.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vitest": "^0.18.1"
  }
}
